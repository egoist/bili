{
  "name": "bili",
  "version": "0.18.2",
  "description": "Schweizer Armeemesser for your JavaScript libraries.",
  "license": "MIT",
  "repository": "egoist/bili",
  "author": {
    "name": "EGOIST",
    "email": "0x142857@gmail.com",
    "url": "github.com/egoist"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "cross-env BABEL_ENV=test jest --env node && npm run lint && npm run build",
    "test:cov": "cross-env BABEL_ENV=test jest --env node --coverage && xo src/*.js",
    "build": "babel src --out-dir lib",
    "dev": "babel src --out-dir lib --watch",
    "lint": "xo",
    "fix": "npm run lint -- --fix",
    "prepublish": "npm run build"
  },
  "main": "lib/bili.js",
  "bin": "cli.js",
  "files": [
    "lib",
    "cli.js"
  ],
  "keywords": [
    "rollup",
    "esnext",
    "compile",
    "babel",
    "buble",
    "es6"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint-config-rem": "^3.1.0",
    "fs-extra": "^4.0.0",
    "jest-cli": "^20.0.4",
    "rimraf": "^2.6.1",
    "rollup-plugin-typescript": "^0.8.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "extends": "rem/prettier",
    "envs": [
      "jest"
    ],
    "rules": {
      "import/no-unresolved": 0
    },
    "ignores": [
      "docs/**"
    ]
  },
  "dependencies": {
    "cac": "^4.2.0",
    "camelcase": "^4.1.0",
    "chalk": "^2.1.0",
    "fancy-log": "^1.2.0",
    "lodash": "^4.17.4",
    "req-cwd": "^2.0.0",
    "rollup": "^0.49.2",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "stringify-author": "^0.1.3",
    "switchy": "^0.1.0",
    "update-notifier": "^2.2.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/"
  }
}
