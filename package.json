{
  "name": "bili",
  "version": "1.3.8",
  "description": "Delightful library bundler.",
  "repository": {
    "url": "egoist/bili",
    "type": "git"
  },
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "files": [
    "lib",
    "babel.js"
  ],
  "scripts": {
    "test": "npm run lint && jest",
    "test:cov": "npm run lint && jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier-eslint src/**/*.js --write",
    "prepublishOnly": "npm run build",
    "build": "babel src -d lib",
    "watch": "babel src -d lib -w"
  },
  "author": "EGOIST <0x142857@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-alter-object-assign": "^1.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-vue": "^2.0.0",
    "boxen": "^1.3.0",
    "cac": "^4.2.4",
    "camelcase": "^4.1.0",
    "chalk": "^2.3.0",
    "fast-async": "^6.3.0",
    "first-commit-date": "^0.2.0",
    "globby": "^7.1.1",
    "gzip-size": "^4.1.0",
    "pretty-bytes": "^4.0.2",
    "read-pkg-up": "^3.0.0",
    "rollup": "^0.53.2",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-hashbang": "^1.0.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "string-width": "^2.1.1",
    "stringify-author": "^0.1.3",
    "text-table": "^0.2.0",
    "uglify-es": "^3.3.4",
    "use-config": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "eslint": "4.15.0",
    "eslint-config-rem": "3.2.2",
    "eslint-config-xo": "0.19.0",
    "jest": "22.0.4",
    "prettier-eslint-cli": "4.7.0"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "rem"
    ],
    "env": {
      "jest": true
    }
  },
  "eslintIgnore": [
    "dist/**"
  ],
  "jest": {
    "testEnvironment": "node"
  }
}
